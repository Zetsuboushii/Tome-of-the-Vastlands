---
layout: none
---
[
{% assign counter = 0 %}
{% for c in site.entries %}
{% if c.categories contains "calendar" %}
{% if counter > 0 %},{% endif %}
{
"name": "{{ c.title }}",
{% assign date = c.when | split: "-" %}
"date": "{{ date[1] }}.{{ date[0] }}",
"type": "",
{% assign content_parts = c.content | split: '<h3' %}
{% assign excerpt = c.excerpt | replace: "\r\n", " " | replace: "\r", " " | replace: "\n", " " %}
"content": {
"excerpt": "{{ excerpt | strip_html | strip }}",
"sections": [
{% for part in content_parts offset: 1 %}
{% assign section_title = part | split: '</h3' | first | split: '>' | last | strip_html | strip %}
{% assign section_content = part | split: '</h3>' | last | split: '</h3>' | first | strip_html | strip | replace: "\r\n", " " | replace: "\n\r", " " | replace: "\r", " " | replace: "\n", " " %}
{
"title": "{{ section_title }}",
"text": "{{ section_content }}"
}{% unless forloop.last %},{% endunless %}
{% endfor %}
]
}
}{% assign counter = counter | plus: 1 %}
{% endif %}
{% endfor %}
]